
<% contents.each do |content| %>

    

    <%= form_for content, url: edit_lesson_path(lesson) do |f| %>
        <%= f.label :type, "Type" %><br>
        <%= f.select :data_type, Content.data_types %><br>
        
        <%= f.label :data, "Content" %><br>
        <%= f.text_area :data %><br>

        <%= f.submit %>
    <% end %>


<% end %>